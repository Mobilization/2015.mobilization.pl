 <!-- address -->
 <div id="when-where">
  <div class="container text-center wow bounceInLeft">
  	<% event = @site.event %>
    <p class="lead"><%= event.data %>, <%= event.time %><p>
    <p>
      <%= event.location.place %>,<br />
      <%= event.location.street %><br />
   	  <%= event.location.zip %> <%= event.location.city %>, <%= event.location.country %>
    </p>
  </div>
</div>
